.video_container {
  margin: 0;
  position: relative;
  overflow: hidden;
  width: 100%;
  padding-top: 56.25%; /* 16:9 Aspect Ratio (divide 9 by 16 = 0.5625) */
}

iframe {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 100%;
}
.theme-default-content:not(.custom) {
  max-width: 1080px;
  min-width: 960px;
  width: auto;
}
@media screen and (max-width: 419px) {
  .theme-default-content:not(.custom) {
    max-width: auto;
    min-width: auto;
    width: auto;
  }
}
.nav-links .icon.outbound {
  display: none;
}
.nav-links .nav-item a:hover {
  border-bottom: 2px solid #46bd87;
}
@media screen and (max-width: 719px) {
  .navbar a .can-hide {
    display: inline-block;
  }
}
