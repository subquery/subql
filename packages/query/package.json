    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.17.20",
    "@types/ws": "^8",
    "@types/yargs": "^16.0.9",
    "nodemon": "^3.1.4"
  }
